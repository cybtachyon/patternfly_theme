{#
/**
 * @file
 * Theme override to display primary and secondary local tasks.
 *
 * Available variables:
 * - primary: HTML list items representing primary tasks.
 * - secondary: HTML list items representing primary tasks.
 *
 * Each item in these variables (primary and secondary) can be individually
 * themed in menu-local-task.html.twig.
 */
#}
{% if primary %}
  {% embed '@patternfly/components/navigation/navigation.html.twig' with {
    'layout': 'pf-c-nav__tertiary-list',
    'label': 'Primary tabs'|t,
    } %}
    {% block navitems %}
      {{ primary }}
    {% endblock %}
  {% endembed %}
{% endif %}
{% if secondary %}
  {% embed '@patternfly/components/navigation/navigation.html.twig' with {
    'layout': 'pf-c-nav__tertiary-list',
    'label': 'Secondary tabs'|t,
    } %}
    {% block navitems %}
      {{ secondary }}
    {% endblock %}
  {% endembed %}
{% endif %}
